<% provide(:title, 'creat threemonths_goal') %>
<% provide(:button, '作成') %>
<h1>３ヶ月ごとの目標</h1>
<div class="row">
  <div class="col-md-8 col-md-offset-2">
  <%= form_with model: [@users, @threemonths_goals] do |f| %>
    <%= f.fields_for : threemonths_goals do |i| %>
    <div class="field">
      <%= f.text_field :content %>
    </div>
    <% end %>

    <% end %>
    <%= i.submit "Submit" %>

  <% end %>
  </div>
  <div class="col-md-1 col-md-offset-1">
    <%= link_to "キャンセル", root_path , class: "btn" , id: "cancel" %>
  </div>
</div>
